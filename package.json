    "minidenticons": "0.1.8",
    "moment": "2.29.1",
    "qrcode.react": "1.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-minimal-pie-chart": "8.2.0",
    "react-redux": "^7.2.3",
    "react-tooltip": "4.2.15",
