    "regenerator-runtime": "^0.13.5",
    "reselect": "^4.0.0",
    "satoshi-bitcoin": "1.0.5",
    "source-map-support": "^0.5.19"
  },
  "engines": {
    "node": ">=15.x",
    "npm": ">=6.x",
    "yarn": ">=1.21.3"
  },
  "browserslist": [],
  "renovate": {
    "extends": ["bliss"],
    "baseBranches": ["next"]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn build && jest"
    }
  }
}
